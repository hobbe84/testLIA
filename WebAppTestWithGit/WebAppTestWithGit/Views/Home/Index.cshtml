        <li data-target="#myCarousel" data-slide-to="3"></li>
        <li data-target="#myCarousel" data-slide-to="4"></li>
    </ol>

    <!-- Wrapper for Slides -->
    <div class="carousel-inner">
        <div class="item active">
            <!-- Set the first background image using inline CSS below. -->
            <div class="fill" style="background-image:url('https://scontent-ams.xx.fbcdn.net/hphotos-xta1/v/l/t1.0-9/p480x480/10365735_10203709441966384_4165648462281473328_n.jpg?oh=d88ce72b87de3cdeb6d67b1890574b37&oe=55A1E019');"></div>
            <div class="carousel-caption">
                <h2>3 studenter på LIA...</h2>
            </div>
        </div>

        <div class="item">
            <!-- Set the second background image using inline CSS below. -->           
            <div class="fill" style="background-image:url('https://fbcdn-sphotos-f-a.akamaihd.net/hphotos-ak-xpa1/v/t1.0-9/1149013_10151536345765906_1798697312_n.jpg?oh=0df6f0c0543b154919a878d672fe8150&oe=55D16C54&__gda__=1436752011_707a8d894497b6e3499f51933589a030');"></div>
            <div class="carousel-caption">
                <h2>3 studenter på LIA...</h2>
            </div>
        </div>

        <div class="item">
            <!-- Set the second background image using inline CSS below. -->
            <div class="fill" style="background-image:url('../pictures/bild_ag.jpg');"></div>
            <div class="carousel-caption">
                <h2>3 studenter på LIA...</h2>
            </div>
        </div>

        <div class="item">
            <!-- Set the second background image using inline CSS below. -->
            <div class="fill" style="background-image:url('http://i.ytimg.com/vi/EFhii04Piww/maxresdefault.jpg');"></div>
            <div class="carousel-caption">
                <h2>Hur ska detta gå</h2>
